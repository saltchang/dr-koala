{
  "cSpell.words": [
    "agentchat",
    "autogen",
    "datname",
    "levelname",
    "llms",
    "modelcontextprotocol",
    "ondelete",
    "onupdate",
    "PKCS",
    "poolclass",
    "SECP",
    "selectin",
    "tabler"
  ],
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": true,
    "editor.wordBasedSuggestions": "off",
    "editor.codeActionsOnSave": {
      "source.fixAll": "always"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "plpgsqlLanguageServer.enableExecuteFileQueryCommand": false,
  "plpgsqlLanguageServer.workspaceValidationTargetFiles": [],
  "cursorpyright.analysis.autoImportCompletions": true,
  "cursorpyright.analysis.autoSearchPaths": true,
  "python.languageServer": "Pylance",
  "ruff.enable": true,
  "ruff.organizeImports": true,
  "ruff.fixAll": true,
  "biome.lsp.bin": "frontend/node_modules/.bin/biome",
  "biome.enabled": true,
  "biome.configurationPath": "frontend/biome.json",
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "less.lint.unknownAtRules": "ignore"
}
